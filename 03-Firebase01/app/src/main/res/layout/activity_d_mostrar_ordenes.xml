<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DMostrarOrdenes">

    <Button
        android:id="@+id/btn_mostrarOrdenes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:text="Mostrar"
        app:layout_constraintEnd_toEndOf="@+id/lv_mostrarOrdenes"
        app:layout_constraintStart_toStartOf="@+id/lv_mostrarOrdenes"
        app:layout_constraintTop_toBottomOf="@+id/lv_mostrarOrdenes" />

    <ListView
        android:id="@+id/lv_mostrarOrdenes"
        android:layout_width="351dp"
        android:layout_height="449dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>